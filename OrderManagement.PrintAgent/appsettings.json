{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/print-agent-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "PrintAgent": {
    "WebSocketPort": 9100,
    "ApiBaseUrl": "https://localhost:5001/api/v1",
    "TenantCode": "DINEHUB",
    "BranchCode": "HQ",
    "SyncIntervalMinutes": 5,
    "DefaultPaperWidth": 80
  }
}

